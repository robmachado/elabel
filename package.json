{
  "name": "elabel",
  "version": "0.0.1",
  "description": "Zebra Label Printing",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-linux": "electron-packager . elabel --overwrite --platform=linux --arch=x64 --ignore=oldfiles --icon=assets/icons/elabel.png --prune=true --out=release-builds",
    "package-win": "electron-packager . elabel --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/elabel.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Elabel App\""
  },
  "repository": "https://github.com/robmachado/elabel",
  "keywords": [
    "Electron",
    "Zebra",
    "Label",
    "Extruder"
  ],
  "author": "Roberto L. Machado",
  "license": "MIT",
  "devDependencies": {
    "asar": "^0.12.4",
    "electron": "^1.4.1",
    "electron-builder": "^10.15.1",
    "electron-packager": "^8.4.0",
    "electron-prebuilt": "^1.4.13"
  },
  "dependencies": {
    "dotenv": "^2.0.0",
    "moment": "^2.17.1",
    "mysql": "^2.12.0",
    "nanotimer": "^0.3.14",
    "node-printer": "^1.0.2"
  }
}
